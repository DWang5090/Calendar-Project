
public class Calendar {
	
	private String classification;
	
	public Calendar(String classification){
		
	}
	
	public boolean checkClassification(String classification){
		this.classification = classification.toUpperCase();
	    if(classification.toUpperCase().equals("PUBLIC") || classification.toUpperCase().equals("PRIVATE") || classification.toUpperCase().equals("CONFIDENTIAL")){
	    	return true;
	    }
	    return false;
	}
	
	public void setClassification(String classification){
		this.classification = classification;
	}
	public String getClassification(){
		return classification;
	}
}
